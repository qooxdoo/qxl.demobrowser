{
	"$schema": "https://qooxdoo.org/schema/compile-1-0-0.json",
	"environment": {
		"qx.allowUrlVariants": true,
		"qx.allowUrlSettings": true,
		"qx.contrib": false,
		"qx.icontheme": ["Tango", "Oxygen"]
	},
	"applications": [{
		"class": "qxl.demobrowser.Application",
		"theme": "qxl.demobrowser.Theme",
		"name": "demobrowser",
		"title": "DemoBrowser",
		"bootPath": "source/boot",
		"include": [
			"qxl.demobrowser.*",
			"qx.theme.Indigo",
			"qx.theme.Modern",
			"qx.theme.Simple",
			"qx.theme.Classic"
		]
	}],
	/** Targets */
	"targets": [{
			"type": "source",
			"outputPath": "compiled/source",
			"bundle": {
				"include": "qx.*"
   		},
	  	"babelOptions": {
				"targets": {
						"edge": "18",
						"chrome": "73",
						"firefox": "66"
			  }
		  }
		},
		{
			"type": "build",
			"outputPath": "compiled/build"
		}
	],
	"defaultTarget": "source",
	"locales": [
		"en"
	],
	"serve": {
		"listenPort": 8080
	}
}